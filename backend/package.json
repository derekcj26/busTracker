{
  "name": "bustracker-backend",
  "version": "1.0.0",
  "description": "Backend for BusTracker project using TypeScript and GTFS-realtime protobufs.",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc --build ../tsconfig.json",
    "build:watch": "tsc --build --watch ../tsconfig.json",
    "start": "node dist/index.js",
    "dev": "concurrently \"npm:build:watch\" \"npm:start\""
  },
  "dependencies": {
    "express": "^4.18.2",
    "axios": "^1.6.7",
    "protobufjs": "^7.2.5"
  },
  "devDependencies": {
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "@types/node": "^20.11.30",
    "@types/express": "^4.17.21",
    "protobufjs-cli": "^1.1.1",
    "concurrently": "^8.2.2"
  }
}
